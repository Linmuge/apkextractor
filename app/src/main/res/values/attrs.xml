<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- SwitchView 自定义属性 -->
    <declare-styleable name="SwitchView">
        <!-- 开关标题 -->
        <attr name="title" format="string" />
        <!-- 开关描述（可选） -->
        <attr name="desc" format="string" />
        <!-- 左侧图标（可选） -->
        <attr name="icon" format="reference" />
    </declare-styleable>
</resources>

